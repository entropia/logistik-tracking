{
	"name": "frontend-ang-2",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"gen_sources": "yarn run gen_svelte && yarn run gen_graphql && yarn run gen_openapi",
		"gen_svelte": "svelte-kit sync",
		"gen_graphql": "graphql-codegen -c codegen.ts -o true",
		"gen_openapi": "openapi-typescript ../api/api.yaml -o ./src/gen/openapi.d.ts",
		"dev": "yarn run gen_sources && vite dev -m development",
		"build": "yarn run gen_sources && vite build -m production && cp -v package.json build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:ts": "tsc --noEmit"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^6.0.0",
		"@graphql-codegen/client-preset": "^5.0.0",
		"@graphql-codegen/typescript": "^5.0.0",
		"@graphql-codegen/typescript-operations": "^5.0.0",
		"@internationalized/date": "^3.8.1",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"bits-ui": "^2.11.0",
		"clsx": "^2.1.1",
		"openapi-typescript": "^7.10.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.2.2",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"packageManager": "yarn@4.9.4",
	"dependencies": {
		"@m31coding/fuzzy-search": "^1.0.2",
		"@sveltejs/adapter-node": "^5.3.1",
		"@sveltejs/adapter-static": "^3.0.9",
		"@tailwindcss/vite": "^4.1.13",
		"graphql": "^16.11.0",
		"openapi-fetch": "^0.15.0",
		"svelte-persisted-store": "^0.12.0",
		"tailwindcss": "^4.1.13"
	}
}
